{
  "name": "iapunto-website",
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "start": "node ./dist/server/entry.mjs",
    "dev": "astro dev",
    "build": "astro build --config astro.config.railway.mjs",
    "preview": "astro preview --config astro.config.railway.mjs --port ${PORT:-4321}",
    "deploy": "pnpm run build",
    "astro": "astro",
    "format": "pnpm exec prettier --write .",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-images:watch": "node scripts/optimize-images.js --watch",
    "article:create": "tsx scripts/automated-article-creator.ts",
    "article:monitor": "tsx scripts/article-monitor.ts",
    "article:setup": "tsx scripts/setup-automation.ts setup",
    "article:test": "tsx scripts/setup-automation.ts test",
    "article:test-flow": "tsx scripts/test-gem-flow.ts",
    "dashboard:test": "tsx scripts/test-dashboard.ts",
    "env:setup": "tsx scripts/setup-env.ts",
    "db:test": "tsx scripts/setup-database.ts --test-only",
    "db:update-url": "tsx scripts/update-db-url.ts",
    "gemini:fix": "tsx scripts/fix-gemini-api.ts",
    "article:enable": "tsx scripts/setup-automation.ts enable",
    "article:disable": "tsx scripts/setup-automation.ts disable",
    "article:stats": "curl -s http://localhost:4321/api/articles/create-automatic | jq .",
    "article:health": "tsx scripts/article-monitor.ts --health-check",
    "db:setup": "tsx scripts/setup-database.ts",
    "railway:setup": "tsx scripts/railway-setup-final.ts",
    "railway:db": "tsx scripts/railway-database-manager.ts",
    "build:coolify": "astro build --config astro.config.coolify.mjs",
    "build:coolify:working": "astro build --config astro.config.coolify.working.mjs",
    "build:simple": "astro build --config astro.config.simple.mjs",
    "start:coolify": "node ./dist/server/entry.mjs",
    "coolify:migrate": "tsx scripts/migrate-to-coolify.ts",
    "coolify:setup": "tsx scripts/migrate-to-coolify.ts --no-backup",
    "articles:move-malformed": "node scripts/move-malformed-articles.cjs"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.1.0",
