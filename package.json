    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "astro": "^5.4.2",
    "cloudinary": "^2.7.0",
    "dotenv": "^16.6.1",
    "flagpack": "^1.0.5",
    "flatpickr": "^4.6.13",
    "flowbite": "^2.5.2",
    "googleapis": "^155.0.1",
    "gray-matter": "^4.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "path-to-regexp": "^8.2.0",
    "pg": "^8.16.3",
