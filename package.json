    "lodash-es": "^4.17.21",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "react-email": "^4.1.1",
    "sass": "^1.85.1",
    "sass-embedded": "^1.85.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2"
  }
